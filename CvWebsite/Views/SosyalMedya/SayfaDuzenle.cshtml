@model CvWebsite.Models.Entity.TbSocial 
@{
    ViewBag.Title = "SayfaDuzenle";
    Layout = "~/Views/Shared/_MainLayout.cshtml";
}
<form method="post">
    @Html.Label("İsim")
    @Html.TextBoxFor(x => x.Name, new { @class = "form-control" })
    <br />
    @Html.Label("Link")
    @Html.TextBoxFor(x => x.Link, new { @class = "form-control" })
    <br />
    @Html.Label("İkon")
    @Html.TextBoxFor(x => x.Icon, new { @class = "form-control" })
    <br />
    @Html.Label("Durum")
    @Html.TextBoxFor(x => x.Durum, new { @class = "form-control" })
   
    <button class="btn btn-dark">Düzenle</button>
</form>
